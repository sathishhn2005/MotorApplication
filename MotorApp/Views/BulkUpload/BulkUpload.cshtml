                                    return false;
                                }                              
                                if (cells[11] == "") {
                                    $('#fileUploadMotorPolicies').val('');
                                    alert("Please check Row NO : " + RRC + " Customer Name is empty");
                                    return false;
                                }
                                if (cells[12] == "") {
                                    $('#fileUploadMotorPolicies').val('');
                                    alert("Please check Row NO : " + RRC + " Source Code is empty");
