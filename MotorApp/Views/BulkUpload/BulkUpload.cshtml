                                    return false;
                                }

                                if (cells[6] == "") {
                                    $('#fileUploadMotorPolicies').val('');
                                    alert("Please check Row NO : " + RRC + " Period From is empty");
                                    return false;
                                }
                                else {
                                    //Datevalitation
                                    if (!isValidatedate(cells[6])) {

                                        $('#fileUploadMotorPolicies').val('');
